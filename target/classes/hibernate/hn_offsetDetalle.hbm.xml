<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.artiffex.lithomat.sistemaweb.businesstier.entity.OffsetDetalle" table="offset_detalle" dynamic-update="true" optimistic-lock="dirty">
		<id name="idOffsetDetalle">
			<column name="id_offset_detalle"/>
			<type name="integer"/>
			<generator class="increment"/>
		</id>
		<many-to-one name="pliego" class="com.artiffex.lithomat.sistemaweb.businesstier.entity.Pliego" fetch="select">
			<column name="id_pliego"/>
		</many-to-one>
		<property name="hojasBuenas">
			<column name="hojas_buenas"/>
			<type name="integer"/>
		</property>
		<property name="hojasMalas">
			<column name="hojas_malas"/>
			<type name="integer"/>
		</property>
		<property name="hojasLimpias">
			<column name="hojas_limpias"/>
			<type name="integer"/>
		</property>
		<property name="hojasAdicionales">
			<column name="hojas_adicionales"/>
			<type name="integer"/>
		</property>
		<property name="laminasExtras">
			<column name="laminas_extras"/>
			<type name="integer"/>
		</property>
		<property name="kilosTintaFrente">
			<column name="kilos_tinta_frente"/>
			<type name="integer"/>
		</property>
		<property name="kilosTintaVuelta">
			<column name="kilos_tinta_vuelta"/>
			<type name="integer"/>
		</property>
		<property name="activo">
			<column name="activo"/>
			<type name="integer"/>
		</property>
	</class>
</hibernate-mapping>