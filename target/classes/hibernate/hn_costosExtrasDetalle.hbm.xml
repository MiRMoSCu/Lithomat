<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.artiffex.lithomat.sistemaweb.businesstier.entity.CostosExtrasDetalle" table="costos_extras_detalle" dynamic-update="true" optimistic-lock="dirty">
		<id name="idCostosExtrasDetalle">
			<column name="id_costos_extras_detalle"/>
			<type name="integer"/>
			<generator class="increment"/>
		</id>
		<many-to-one name="partida" class="com.artiffex.lithomat.sistemaweb.businesstier.entity.Partida" fetch="select">
			<column name="id_partida"/>
		</many-to-one>
		<many-to-one name="responsableInsumo" class="com.artiffex.lithomat.sistemaweb.businesstier.entity.ResponsableInsumo" fetch="select">
			<column name="id_responsable_insumo"/>
		</many-to-one>
		<many-to-one name="costosExtras" class="com.artiffex.lithomat.sistemaweb.businesstier.entity.CostosExtras" fetch="select">
			<column name="id_costos_extras"/>
		</many-to-one>
		<property name="cantidad">
			<column name="cantidad"/>
			<type name="integer"/>
		</property>
		<property name="especificaciones">
			<column name="especificaciones" length="200"/>
			<type name="string"/>
		</property>
		<property name="precioTotalPesos">
			<column name="precio_total_pesos"/>
			<type name="float"/>
		</property>
		<property name="activo">
			<column name="activo"/>
			<type name="boolean"/>
		</property>
	</class>
</hibernate-mapping>