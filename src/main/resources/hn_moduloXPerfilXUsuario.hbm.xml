<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.artiffex.lithomat.sistemaweb.businesstier.entity.ModuloXPerfilXUsuario" table="modulo_x_perfil_x_usuario" dynamic-update="true" optimistic-lock="dirty">
		<id name="idModuloXPerfilXUsuario">
			<column name="id_modulo_x_perfil_x_usuario"/>
			<type name="integer"/>
			<generator class="increment"/>
		</id>
		<many-to-one name="usuario" class="com.artiffex.lithomat.sistemaweb.businesstier.entity.Usuario" fetch="select">
			<column name="id_usuario"/>
		</many-to-one>
		<many-to-one name="perfil" class="com.artiffex.lithomat.sistemaweb.businesstier.entity.Perfil" fetch="select">
			<column name="id_perfil"/>
		</many-to-one>
		<many-to-one name="modulo" class="com.artiffex.lithomat.sistemaweb.businesstier.entity.Modulo" fetch="select">
			<column name="id_modulo"/>
		</many-to-one>
		<many-to-one name="subModulo" class="com.artiffex.lithomat.sistemaweb.businesstier.entity.SubModulo" fetch="select">
			<column name="id_sub_modulo"/>
		</many-to-one>
		<property name="tieneAcceso">
			<column name="tiene_acceso"/>
			<type name="boolean"/>
		</property>
		<property name="permisoAlta">
			<column name="permiso_alta"/>
			<type name="boolean"/>
		</property>
		<property name="permisoBaja">
			<column name="permiso_baja"/>
			<type name="boolean"/>
		</property>
		<property name="permisoCambio">
			<column name="permiso_cambio"/>
			<type name="boolean"/>
		</property>
		<property name="permisoConsulta">
			<column name="permiso_consulta"/>
			<type name="boolean"/>
		</property>
		<property name="activo">
			<column name="activo"/>
			<type name="boolean"/>
		</property>
	</class>
</hibernate-mapping>